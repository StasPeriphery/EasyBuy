@using  EasyBuyShop.Models
@model IEnumerable<Product>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <nav>
        <div class="head">
            <a class="navigation" href="Home/SignIn">Pricing</a>
            <a class="navigation" href="#">Cart</a>
        </div>
    </nav>
    <div class="basis">
        <div class="panel_search"><h1>panel_search</h1> </div>
        <div class="table">
            <h1>table</h1>
            <div>
                @{
                    foreach (var cell in Model)
                    {
                        <div class="item_table">
                            <h2> @cell.heading</h2>
                            <img class="img_cell" src="@cell.url" alt="@cell.nameProduct" />
                            <h3>Price: @cell.price</h3>
                            <h3>Price: @cell.nameProduct</h3>
                        </div>
                    }
                }
            </div>
        </div>
    </div>
    <footer></footer>
    @*<script src="~/js/bootstrap.min.js"></script>*@
</body>
</html>